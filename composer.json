{
    "name": "kheme/laravel-supertext-nigeria",
    "description": "A Laravel package for SuperText Nigeria's SMS API",
    "keywords": [
        "kheme",
        "laravel",
        "sms",
        "supertextng"
    ],
    "homepage": "https://github.com/kheme/laravel-supertext-nigeria",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Okiemute Omuta",
            "email": "iamkheme@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Kheme\\SuperTextNg\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.3.0"
    },
    "repositories": {
        "package-name": {
            "type": "path",
            "url": "packages/kheme/laravel-supertext-nigeria",
            "options": {
                "symlink": true
            }
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kheme\\SuperTextNg\\SuperTextNgServiceProvider"
            ],
            "aliases": {
                "SMS": "Kheme\\SuperTextNg\\Facades\\SMS"
            }
        }
    }
}
