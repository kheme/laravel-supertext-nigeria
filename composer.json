{
    "name"       : "kheme/laravel-supertext-nigeria",
    "description": "A Laravel package for SuperText Nigeria's SMS API",
    "version"    : "1.0.0",
    "keywords"   : [
        "kheme",
        "laravel",
        "sms",
        "laravel sms",
        "supertextng",
        "sms gateway",
        "laravel sms gateway",
        "laravel sms sender"
    ],
    "homepage": "https://github.com/kheme/laravel-supertext-nigeria",
    "type"    : "package",
    "license" : "MIT",
    "authors" : [
        {
            "name"    : "Okiemute Omuta",
            "email"   : "iamkheme@gmail.com",
            "homepage": "https://github.com/kheme",
            "role"    : "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Kheme\\SuperTextNg\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable"    : true,
    "require"          : {
        "php": ">=5.3.0"
    },
    "repositories": {
        "package-name": {
            "type"   : "path",
            "url"    : "packages/kheme/laravel-supertext-nigeria",
            "options": {
                "symlink": true
            }
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kheme\\SuperTextNg\\SuperTextNgServiceProvider"
            ],
            "aliases": {
                "SMS": "Kheme\\SuperTextNg\\Facades\\SMS"
            }
        }
    }
}
