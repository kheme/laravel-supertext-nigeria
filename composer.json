{
    "name": "kheme/laravel-supertext-nigeria",
    "description": "A Laravel package for SuperText Nigeria's API",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Okiemute Omuta",
            "email": "iamkheme@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Kheme\\SuperTextNg\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": "^7.0"
    },
    "repositories": {
        "package-name": {
            "type": "path",
            "url": "packages/kheme/laravel-supertext-nigeria",
            "options": {
                "symlink": true
            }
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "SuperTextNg": "Kheme\\SuperTextNg\\Facade\\SuperTextNg"
            }
        }
    }
}
